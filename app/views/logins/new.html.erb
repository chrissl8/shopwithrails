<div class="form-item">
  <h1>ShopWithRails &#9733; Log In</h1>
  <%= form_tag '/login' do %>
  <p>
  <%= label_tag :username %><br>
  <%= text_field_tag :username %>
  </p>
  <p>
  <%= label_tag :password %><br>
  <%= password_field_tag :password %>
  </p>
  <%= submit_tag 'Log In' %>
  
  <% end %>
</div>